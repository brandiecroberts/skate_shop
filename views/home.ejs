            </div>
            <div class="max">
              <label>Max</label><span id="max-value">
              </span>
            </div>
          </div>

        <div class="min-max-range">
          <input type="range" min="0" max="5000"
          value="2000" class="range" id="min">
          <input type="range" min="5001" max="10000"
          value="8000" class="range" id="max">
        </div>

